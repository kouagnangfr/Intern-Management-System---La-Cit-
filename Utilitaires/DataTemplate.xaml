<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:StagiaireLaCite.VuesModels"
                    xmlns:vue="clr-namespace:StagiaireLaCite.Vues">

    <DataTemplate DataType="{x:Type vm:AccueilVM}">
        <vue:Accueil/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ConsulterVM}">
        <vue:Consulter/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ParametresVM}">
        <vue:Parametres/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ProgrammesVM}">
        <vue:Programmes/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:StagiairesVM}">
        <vue:Stagiaires/>
    </DataTemplate>
    
    
</ResourceDictionary>